    "package": "pkg . --targets node8-linux-x64,node8-macos-x64 --out-path ./deployables"
  },
  "config": {
    "port": "3010",
    "noncePreference": "blockchain"
  },
  "author": "miro",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^1.18.3"
  },
  "dependencies": {
    "command-line-args": "4.0.7",
    "ethereumjs-tx": "1.3.3",
    "ethereumjs-units": "0.2.0",
    "ethereumjs-util": "5.1.3",
    "etherscan-api": "^10.0.2",
    "express": "4.16.2",
    "lodash": ">=4.17.21",
    "npm": "^6.11.1"
  }
}
