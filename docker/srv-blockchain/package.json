{
  "name": "blockchainapi",
  "version": "0.0.1",
  "description": "API for storing hashes in a Blockchain",
  "main": "server.js",
  "bin": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
